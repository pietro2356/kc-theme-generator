{
  "name": "angular-workspace-template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --watch --open",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format:test": "prettier --list-different  \"./projects/**/src/**/*.{ts,html,css,scss,json}\"",
    "format:write": "prettier --write \"./projects/**/src/**/*.{ts,html,css,scss,json}\"",
    "lint": "ng lint",
    "analyze": "ng build --stats-json --output-hashing none --named-chunks && esbuild-visualizer --template treemap --metadata dist/app-template/stats.json --filename dist/app-template/analyze/index.html && http-server -o -c-1 ./dist/app-template/analyze/",
    "analyze:sme": "ng build --stats-json --output-hashing none --named-chunks && source-map-explorer dist/app-template/browser/*.js --html dist/app-template/sme/index.html && http-server -o -c-1 ./dist/app-template/sme/",
    "analyze:deps": "npm-run-all analyze:deps:*",
    "analyze:deps:all": "madge projects/app-template/src/main.ts --ts-config tsconfig.json --image ./deps/app-template/_all.png",
    "analyze:deps:ui": "madge --extensions ts --ts-config tsconfig.json --image ./deps/app-template/ui.png projects/app-template/src/app/ui/",
    "analyze:deps:core": "madge --extensions ts --ts-config tsconfig.json --image ./deps/app-template/core.png projects/app-template/src/app/core/",
    "analyze:deps:layout": "madge --extensions ts --ts-config tsconfig.json --image ./deps/app-template/layout.png projects/app-template/src/app/layout/",
    "analyze:deps:pattern": "madge --extensions ts --ts-config tsconfig.json --image ./deps/app-template/pattern.png projects/app-template/src/app/pattern/",
    "analyze:deps:feature": "madge --extensions ts --ts-config tsconfig.json --image ./deps/app-template/feature.png projects/app-template/src/app/feature/"

  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.6",
    "@angular/common": "^17.3.6",
    "@angular/compiler": "^17.3.6",
    "@angular/core": "^17.3.6",
    "@angular/forms": "^17.3.6",
    "@angular/platform-browser": "^17.3.6",
    "@angular/platform-browser-dynamic": "^17.3.6",
    "@angular/router": "^17.3.6",
    "primeng": "^17.15.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.6",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "^17.3.6",
    "@angular/compiler-cli": "^17.3.6",
    "@types/jasmine": "~5.1.4",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "esbuild-visualizer": "^0.6.0",
    "eslint": "^9.1.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-boundaries": "^4.2.0",
    "http-server": "^14.1.1",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "madge": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.4.5"
  }
}
